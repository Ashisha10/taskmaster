{
  "family": "taskmaster-task-stage",
  "executionRoleArn": "arn:aws:iam::241533153259:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::241533153259:role/ecsTaskRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "memory": "512",
  "cpu": "256",
  "containerDefinitions": [
    {
      "name": "taskmaster-container-stage",
      "image": "241533153259.dkr.ecr.us-west-1.amazonaws.com/taskmaster-repo-stage:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ]
    }
  ]
}

